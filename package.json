{
  "name": "admin-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "getApiSchema": "node ./custom-scripts/getApiSchema.cjs",
    "generate": "npx openapi-typescript ./src/api/api-schema.yaml -o ./src/api/v1.d.ts",
    "test:ts": "tsc --noEmit",
    "format": "prettier --write ./src",
    "codegen": "openapi-rq -i ./src/api/api-schema.yaml -o ./src/api/generated"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@tanstack/react-query": "^5.62.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "formik": "^2.4.6",
    "imask": "^7.6.1",
    "lucide-react": "^0.468.0",
    "openapi-react-query": "^0.2.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.0.2",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7"
